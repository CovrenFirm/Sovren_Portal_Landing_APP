{
  "title": "Voice Pipeline (TTS + Duplex)",
  "uid": "sovren-voice-pipeline",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "TTS Tier-1 Kokoro (g367:8200)",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "up{job=\"tts-tier1-kokoro\"}",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "value": "1",
              "text": "HEALTHY"
            },
            {
              "type": "value",
              "value": "0",
              "text": "DOWN"
            }
          ]
        }
      }
    },
